# for loop
snippet for
	for (var ${2:i} = 0, l = ${1:arr}.length; $2 < l; $2++) {
		var ${3:v} = $1[$2];${0:}
	}

# function
snippet fun
	function ${1:function_name}(${2}) {
			${0}
	}

# arrow funcitons
snippet afun
	(${1}) => {
		${2}	
	}

# variable functions
snippet vfun
	var ${1:name} = function (${2}){
		${3}
	}

#import
snippet imp
	import ${1:Name} from '${2}';

#export
snippet exp
	export ${1:default} ${2:variable name}
