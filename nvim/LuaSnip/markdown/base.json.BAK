    "body": [
      "```mermaid",
      "flowchart TD",
      "    ${1}",
      "```"
    ]
  },
  "node": {
    "prefix": "node",
    "body": [
      "${1:}[${2:}] -->|${3:}| ${4:}[${5:}]"
    ]
  },
  "flowchart": {
    "prefix": "flowchart",
    "body": [
      "```mermaid",
      "flowchart TD",
      "    A[Users] -->|many to many|B[Channels]",
      "    B -->|has many|D[Messages]",
      "```"
    ]
  },
  "erdiagram": {
    "prefix": "erdiagram",
    "body": [
