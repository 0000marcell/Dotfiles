      "    CUSTOMER ||--o{ ORDER : places",
      "    CUSTOMER {",
      "        string name",
      "        string custNumber",
      "        string sector",
      "    }",
      "    ORDER ||--|{ LINE-ITEM : contains",
      "    ORDER {",
      "        int orderNumber",
      "        string deliveryAddress",
      "    }",
      "    LINE-ITEM {",
      "        string productCode",
      "        int quantity",
      "        float pricePerUnit",
      "    }",
      "```"
    ]
  },
  "sequencediagram": {
    "prefix": "sequencediagram",
    "body": [
      "```mermaid",
      "sequenceDiagram",
      "    Alice->>Bob: Hello Bob, how are you ?",
      "    Bob->>Alice: Fine, thank you. And you?",
      "    create participant Carl",
      "    Alice->>Carl: Hi Carl!",
      "    create actor D as Donald",
      "    Carl->>D: Hi!",
      "    destroy Carl",
      "    Alice-xCarl: We are too many",
      "    destroy Bob",
      "    Bob->>Alice: I agree",
      "```"
    ]
  }
