{
  "mermaid start": {
    "prefix": "ms",
    "body": [
      "```mermaid",
      "flowchart TD",
      "    ${1}",
      "```"
    ]
  },
  "mermaid connection": {
    "prefix": "mc",
    "body": [
      "${1:}[${2:}] --> ${3:}[${4:}]"
    ]
  },
  "mermaid detail": {
    "prefix": "md",
    "body": [
      "|${1:}|"
    ]
  },
  "mermaid arrow": {
    "prefix": "ma",
    "body": [
      "-->"
    ]
  },
  "mermaid node": {
    "prefix": "mn",
    "body": [
      "${1:} --> ${2:}[${3:}]"
    ]
  },
  "table": {
    "prefix": "table",
    "body": [
      "| Environment    | URL                                                                      |",
      "|----------------|--------------------------------------------------------------------------|",
      "| qa             | <http://garage.qa.decisivapps.com> target=\"_blank\">Garage QA</a> |",
      "| prod           | <a href=\"http://garage.qa.decisivapps.com\" target=\"_blank\">Garage PROD</a> |",
      "| studebaker     | <http://garage.staging.decisivapps.com>     |"
    ]
  }
}
