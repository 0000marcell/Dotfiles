{
  "crate a array": {
    "prefix": [
      "arr",
      "array"
    ],
    "body": "items=('apple' 'banana' 'cherry' 'date' 'fig')",
    "description": "creates a array"
  },
  "bash": {
    "prefix": "bash",
    "body": [
      "#!/usr/bin/bash"
    ]
  },
  "for": {
    "prefix": "for",
    "body": [
      "for file_path in \"${file_paths[@]}\"; do",
      "    echo \"Processing file: $file_path\"",
      "    git add $file_path",
      "done"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "if [[ \"$val\" == \"marcell\" ]]; then",
      "  echo \"testing\"",
      "fi"
    ]
  },
  "ife": {
    "prefix": "ife",
    "body": [
      "if [[ -z ${1:var} ]]; then",
      "  echo \"${2:}\"",
      "  exit 1",
      "fi"
    ]
  },
  "fun": {
    "prefix": "fun",
    "body": [
      "function ${1:name} {",
      "  ${2:body}",
      "}"
    ]
  }
}