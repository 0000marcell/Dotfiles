snippet Card
	<Card 
		id={module.get('id')}
		img={module.get('image')} 
		descriptionTitle={module.get('name')}
		description={module.get('description')}
		callCb={(id) => (setState, state) => {
			setState(!state);
			state ?
				setExcludeModuleId(excludeModuleId
					.filter(m => m.get('id') !== id)) :
				setExcludeModuleId(excludeModuleId.push(id))
		}}
	/>

