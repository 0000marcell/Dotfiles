# ajax get
snippet ajaxget
	$.ajax({
		url: url, 
		type: "GET",
		headers: {},
		data: {},
		success: function(data) {
			if(typeof data === 'string') 
				data = JSON.parse(data);
			resolve(data);
		},
		error: function(e) {
			if(typeof e === 'object') {
				console.error(JSON.stringify(e));
			}
			if(typeof e === 'string') {
				console.error(JSON.stringify(e));
				e = JSON.parse(e);
			}
			if(e.responseText) {
				e = JSON.parse(e.responseText);
			}
			reject(e);
		}
	}); 
